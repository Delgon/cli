- name: common-services
  taskRef:
    kind: Task
    name: mas-devops-common-services
  params:
    {{ lookup('template', 'taskdefs/common/cli-params.yml.j2') | indent(4) }}
    - name: devops_suite_name
      value: setup-common-services
  when:
    - input: "$(params.skip_common_services)"
      operator: notin
      values: ["true"]
